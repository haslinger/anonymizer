<p id="notice"><%= notice %></p>

<% @downloads.each do |podcast, downloads| %>
  <%= column_chart(downloads.map{|d| [d.name, d.volume.to_f / d.filesize.to_i]},
                   library: {title: {display: true, text: podcast},
                             scales: { xAxes: [{maxBarThickness: 70 }]}}) %>
<% end %>